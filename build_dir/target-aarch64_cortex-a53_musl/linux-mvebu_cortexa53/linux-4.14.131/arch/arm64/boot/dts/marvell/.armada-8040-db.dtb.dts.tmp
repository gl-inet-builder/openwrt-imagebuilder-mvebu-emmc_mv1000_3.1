    cpm_gpio2: gpio@140 {
     compatible = "marvell,armada-8k-gpio";
     offset = <0x140>;
     ngpios = <31>;
     gpio-controller;
     #gpio-cells = <2>;
     gpio-ranges = <&cpm_pinctrl 0 32 31>;
     interrupt-controller;
     interrupts = <0x0 82 4>,
           <0x0 81 4>,
           <0x0 80 4>,
